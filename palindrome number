class Solution:
        def isPalindrome(self, x: int) -> bool:
              x=str(x)
              if(len(x)==0 ) :
                  return False
              elif(len(x)==1):
                  return True
              else :
                 for i in range(len(x)//2) :
                     if x[i::]==x[::-(i+1)]:
                            return True
                     else:
                           return False
        
